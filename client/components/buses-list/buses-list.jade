.left-col
  section.card.buses-list-card
    header.card-header
      h1.card-header-title Route
    .card-body.card-body-scroll
      ul.card-body-stacked-list
        li(ng-repeat="bus in buses track by bus.MonitoredVehicleJourney.VehicleRef")
          h3(ng-bind="bus.MonitoredVehicleJourney.VehicleRef")
          a(ng-click="goToLoc(bus.MonitoredVehicleJourney.VehicleLocation.Latitude,bus.MonitoredVehicleJourney.VehicleLocation.Longitude)") location
